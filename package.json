{
  "name": "mmp-build",
  "version": "0.2.0",
  "description": "Convert markdown-formatted documents to TeX.",
  "main": "built/index.js",
  "type": "module",
  "scripts": {
    "start": "node built/index.js",
    "lint": "eslint src --ext ts",
    "build": "tsc -p ./"
  },
  "repository": "stevenjoezhang/mmp-build",
  "keywords": [
    "markdown",
    "tex"
  ],
  "bin": {
    "mmp": "./bin/mmp.mjs"
  },
  "author": "Mimi <stevenjoezhang@gmail.com> (https://zhangshuqiao.org)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/stevenjoezhang/mmp-build/issues"
  },
  "homepage": "https://github.com/stevenjoezhang/mmp-build#readme",
  "dependencies": {
    "hexo-filter-mathjax": "^0.9.0",
    "hexo-front-matter": "^3.0.0",
    "hexo-pangu": "^0.2.2",
    "highlight.js": "^11.6.0",
    "marked": "4.3.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@next-theme/eslint-config": "0.0.3",
    "@types/marked": "4.3.2",
    "@types/node": "^18.8.1",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "8.24.0",
    "typescript": "^4.8.4"
  }
}
